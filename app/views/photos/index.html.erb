  <h1>All Photos</h1>
  <div class="container">
      <div class="col-md-8">
        <h1>List of Photos</h1>

<div class="thumbnail">
<% @list_of_all_photos.each do |the_photo| %>


<img src= "<%= the_photo.source %>">
<div class="caption"><p>
<%= the_photo.caption  %>
</p>

<div>
  <a href ="/photos/<%= the_photo.id %>" class="btn btn-default btn-block">Show Image Details</a>
<a href ="/photos/<%= the_photo.id %>/edit" class="btn btn-default btn-block">Edit</a>
<a href ="/delete_photo/<%= the_photo.id %>" class="btn btn-default btn-block">Delete</a></div>
<% end %>
</div>
</div>


</div>
</div>
